// provider = "sqlite"
//  provider = "postgresql"

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model user {
  id Int @id @default(autoincrement())
  email String @unique
  name String?
}
